section "List":

	case "simple unordered list":

		in: 
		<ul>
			<li>One</li>
			<li>Two</li>
			<li>Three</li>
		</ul>

		out:
		- One
		- Two
		- Three

	case "nested unordered list":

		in:
		<ul>
			<li>One
				<ul>
					<li>One Oh One</li>
					<li>One Oh Two</li>
				</ul>
			</li>
			<li>Two
				<ul>
					<li>Two Oh One</li>
					<li>Two Oh Two
						<ul>
							<li>Two Oh Two Oh One</li>
							<li>Two Oh Two Oh Two</li>
						</ul>
					</li>
				</ul>
			</li>
			<li>Three</li>
		</ul>

		out:
		- One
		   - One Oh One
		   - One Oh Two
		- Two
		   - Two Oh One
		   - Two Oh Two
		      - Two Oh Two Oh One
		      - Two Oh Two Oh Two
		- Three

	case "should not break on complex DOM":

		in:
		<ul>
			<br><br>
			<li>Test
				<div><div><form><div>Test Wrapped</div><br><br></form><br><br></div></div><br><br>
				<div><br><br>Test 2</div><br><br>
				<ul>
					<br><br>
					<li>Test
						<div><div><form><div>Test Wrapped</div><br><br></form><br><br></div></div><br><br>
						<div><br><br>Test 2</div><br><br>
					</li>
					<br><br>
				</ul>
			</li>
			<br><br>
		</ul>

		out:
		- Test
		Test Wrapped
		Test 2
		   - Test
		Test Wrapped
		Test 2